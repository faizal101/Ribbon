    "discord.js-commando": "https://github.com/Favna/Commando#ribbon",
    "dotenv": "^6.1.0",
    "eightball": "^1.0.1",
    "emoji-regex": "^7.0.1",
    "erlpack": "hammerandchisel/erlpack",
    "fast-levenshtein": "~2.0.4",
    "flesch-kincaid": "^1.0.3",
    "fuse.js": "^3.2.1",
    "igdb-api-node": "^3.1.7",
    "jimp": "0.5.3",
